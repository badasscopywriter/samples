{% endcapture %}


<div class="section alt">
	<div class="row">
		<div class="small-12 medium-6 large-4 columns">
			<img class="box-shadow-subtle" src="{{ left_photo | resize: '400x300#' }}">
			<h4>{{ left_caption }}</h4>
			<div>{{ left_textbody }}</div>
		</div>
